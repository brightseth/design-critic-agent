{
  "functions": {
    "api/analyze.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/test-env",
      "dest": "/api/test-env.js"
    },
    {
      "src": "/api/analyze",
      "dest": "/api/analyze-simple.js"
    },
